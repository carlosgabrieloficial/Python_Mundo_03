lista = []

maior = menor = 0

for contador in range (0,5):
    lista.append(int(input(f"Digite um numero na posição {contador} :")))

    if contador == 0 :
        maior = menor = lista[contador]

    else:
        if lista[contador]>maior:
            maior = lista[contador]

        if list[contador]<menor:
            menor = list[contador]

print(f"Essa é a sua lista {lista}")

print(f"O maior numero foi {maior} na posição ", end=" ")
for i, valor in enumerate(lista):
    if valor == maior:
        print(f"{i}..",end=" ")
print()


print(f"O menor numero foi {menor} na posição ", end=" ")
for i, valor in enumerate(lista):
    if valor == menor:
        print(f"{i}..",end=" ")
print()
